    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.7.0",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "morgan": "^1.9.1",
    "normalize.css": "^8.0.1",
