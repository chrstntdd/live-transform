{
  "name": "live-transform",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV='development' ./node_modules/.bin/webpack-dev-server --color -c webpack.config.ts",
    "build": "cross-env NODE_ENV='production' ./node_modules/.bin/webpack -c webpack.config.ts",
    "build-ssr": "cross-env NODE_ENV='development' SSR='true' ./node_modules/.bin/webpack -c webpack.config.ts && cross-env NODE_ENV='development' SSR='true' yarn start-server",
    "prod-ssr": "cross-env NODE_ENV='production' SSR='true' ./node_modules/.bin/webpack -c webpack.config.ts && cross-env NODE_ENV='production' yarn start-server",
    "start-server": "ts-node -T ./server/index.ts",
    "prod": "yarn build && NODE_ENV='production' yarn start-server"
  },
  "dependencies": {
    "codemirror": "^5.44.0",
    "preact": "^10.0.0-alpha.2",
    "preact-render-to-string": "^5.0.1",
    "prismjs": "^1.15.0",
    "terser": "^3.17.0"
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.72",
    "@types/express": "^4.16.1",
    "@types/node": "^11.11.0",
    "@types/prismjs": "^1.9.1",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^2.0.1",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "html-webpack-plugin": "4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.5.0",
    "morgan": "^1.9.1",
    "normalize.css": "^8.0.1",
    "react-dev-utils": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "typescript": "next",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-modules": "^1.0.0"
  }
}
